{
	"coc.preferences.hoverTarget": "float",
	"signature.target": "float",
	"languageserver": {
		"haskell": {
			"command": "haskell-language-server-wrapper",
			"args": ["--lsp"],
			"rootPatterns": [
				"*.cabal",
				"stack.yaml",
				"cabal.project",
				"package.yaml"
			],
			"filetypes": [
				"hs",
				"lhs",
				"haskell"
			],
			"initializationOptions": {
				"languageServerHaskell": {
				}
			}
		}
	},
	"suggest.noselect": true
}
