"3.7.1 (created: 2013/05/22 20:32:57)

highlight CmdLine background: #151515; color: #d0d0d0; transition: all 0.25s;
highlight CmdLine>* font-family: limey; font-size: 7pt; background-color: #151515; color: #d0d0d0;
highlight CompItem[selected] background: #101010; box-shadow: 0px 0px 1px #CC0;
highlight InfoMsg color: magenta; background-color: transparent; font-family: limey; font-size: 7pt;
highlight Normal transition: all 0.15s; background: #151515; color: #d0d0d0; font-family: limey !important; font-size: 7pt !important;
highlight StatusLine color: gray; background: transparent; font-weight: normal; font-family: limey; font-size: 7pt;
highlight WarningMsg color: red; font-family: limey; font-size: 7pt;
inoremap <C-a> <Insert><C-a><Insert>
set complete=h
set maxitems=5
source! /home/j/.vimperatorrc.local

" vim: set ft=vimperator:
